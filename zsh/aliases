alias vimalias='mvim ~/.zsh/aliases'

alias rvm='nocorrect rvm'
alias node='nocorrect node'
alias npm='nocorrect npm'

alias t='tail -f'
alias v='mvim ./'

alias rp='touch tmp/restart.txt'
alias rc='rails console'
alias lsa='ls -a'
alias cw='compass --watch'

alias gw='gem which'

# https://coderwall.com/p/4tkkpq
alias map="xargs -n1"
