	<key>IDECodeSnippetContents</key>
	<string>+ (NSString *)reuseIdentifier
{
    static NSString *identifier = nil;
    static dispatch_once_t onceToken;
    dispatch_once(&amp;onceToken, ^
    {
        identifier = NSStringFromClass(self);
    });

    return identifier;
}
