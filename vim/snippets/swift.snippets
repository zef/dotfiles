snippet if
	if ${1:condition} {
		${0}
	}

snippet ife
	if ${1:condition} {
		${2}
	} else {
		${3}
	}
snippet il
	if let ${1:name} = $1${2} {
		${3}
	}

snippet pr
	print(${1})

snippet g
	guard ${1:condition} else { return${2} }
snippet gl
	guard let ${1:name} = $1${2} else { return${3} }

snippet st
	struct ${1:name} {
		${2}
	}
snippet en
	enum ${1:name} {
		case ${2}
	}
snippet cl
	class ${1:name} {
		${2}
	}
snippet prot
	protocol ${1:name} {
		${2}
	}

